* {font-family: "Cascadia Mono";}

MainWindow {
    background-color: #313335;
}

QToolBar {
    background-color: #2b2b2b;
}

QPushButton {
    background-color: #cc7832;
    color: white;

    border-width: 1px;
    border-style: solid;
    border-radius: 4px;
    border-color: transparent;

    padding: 1px;
    margin: 1px;
    margin-left: 8px;
}

QPushButton:hover {
    color: black;
    background-color: #cc7832;
}